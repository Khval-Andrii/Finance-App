<h1>Генератор звітів</h1>
<div>
  <form action="action_report" method="GET">
    <label for="first_date">Дата YYY-MM-DD</label>
    <input type="text" id="first_date" name="start_date" placeholder="2023-01-05">
    <br>
    <label for="second_date">Дата YYY-MM-DD</label>
    <input type="text" id="second_date" name="end_date" placeholder="2023-01-15">
    <br>

    <label for="type_operation">Тип операцій</label>
    <input id="type_operation" name="operations" list="type_operation">
    <datalist id="type_operation">
        <option value="Витрати">
        <option value="Доходи">
    </datalist>
    <br>
    <label for="category">Категорія</label>
    <select id="category" name="categories">
    </select>
    <br>
    <button type="submit" id="btn_graph" name="btn_graph" value="graph">Графік</button>
    <button type="submit" id="btn_dates" name="btn_dates" value="dates">По датам</button>
    <button type="reset" id="btn_reset" name="btn_reset" value="reset">Очистити</button>
  </form>
</div>
